{
  "run": [
    {
      "method": "shell.run",
      "params": {
        "daemon": true,
        "message": "conda activate jarviscore && jarviscore web",
        "on": [
          {
            "event": "/http:\\/\\/localhost:5000/",
            "done": true
          }
        ]
      }
    },
    {
      "method": "local.set",
      "params": {
        "url": "{{input.event[0]}}"
      }
    }
  ]
}